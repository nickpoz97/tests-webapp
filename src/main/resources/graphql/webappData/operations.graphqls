type Query{
    getAllTests: [Test!]!
    getAllDomande: [Domanda!]!
    getTestById(data: Date!, orario: LocalTime!, nome: String!): Test
}

type Mutation{
    addTest(testInput: TestInput!): Test
}

input TestInput{
    giornoDelMese: Int!
    mese: Int!
    anno: Int!
    ora: Int!
    minuto: Int!
    nome: String!
    ordineCasuale: Boolean!
    domandeConNumero: Boolean!
    nomeDomande: [String!]!
}

