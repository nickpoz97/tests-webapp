type Query{
    getAllTests: [Test!]!
    getAllDomande: [Domanda!]!
}

type Mutation{
    addTest(testInput: TestInput): TestInsertionMessage
}

input TestInput{
    giornoDelMese: Int!
    mese: Int!
    anno: Int!
    ora: Int!
    minuto: Int!
    nome: String!
    ordineCasuale: Boolean!
    domandeConNumero: Boolean!
    nomeDomande: [String!]!
}
